<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="chartPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1">
  <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="Account">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#onChangePassword" text="Change Password..." />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
          <HBox alignment="CENTER_LEFT">
            <children>
              <ChoiceBox fx:id="chartChooser" prefWidth="150.0">
                <HBox.margin>
                  <Insets right="5.0" />
                </HBox.margin>
              </ChoiceBox>
              <DatePicker fx:id="startDatePicker" maxWidth="100.0" promptText="Start" />
              <DatePicker fx:id="endDatePicker" prefHeight="25.0" prefWidth="100.0" promptText="End">
                <HBox.margin>
                  <Insets left="5.0" />
                </HBox.margin>
              </DatePicker>
              <Button fx:id="searchButton" mnemonicParsing="false" onAction="#onSearch" text="Search">
                <HBox.margin>
                  <Insets left="5.0" />
                </HBox.margin>
              </Button>
              <ProgressBar prefWidth="100.0" progress="0.0">
                <HBox.margin>
                  <Insets left="5.0" />
                </HBox.margin>
              </ProgressBar>
            </children>
          </HBox>
         </children>
      </VBox>
  </top>
</fx:root>
